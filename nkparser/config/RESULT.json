{
    "result_id" : {
        "col_name" : "結果ID",
        "selector" : "null",
        "var_type" : "text",
        "index" : "primary_key"
    },
    "race_id" : {
        "col_name" : "レースID",
        "selector" : "null",
        "var_type" : "text",
        "index": "entity_id"
    },
    "rank" : {
        "col_name" : "着順",
        "selector" : "td.Result_Num",
        "var_type" : "integer",
        "reg" : "\\d+"
    },
    "bracket" : {
        "col_name" : "枠",
        "selector" : "td[class*='Waku']",
        "var_type" : "integer",
        "reg" : "\\d+"
    },
    "horse_number" : {
        "col_name" : "馬番",
        "selector" : "td.Num.Txt_C",
        "var_type" : "integer",
        "reg" : "\\d+"
    },
    "horse_name" : {
        "col_name" : "馬名",
        "selector" : "span.Horse_Name",
        "function": {"set_title": ["horse_name"]},
        "var_type" : "text",
        "reg" : "\\D+"
    },
    "horse_id" : {
        "col_name" : "馬ID",
        "selector" : "span.Horse_Name",
        "function": {"set_url": ["horse_id"]},
        "var_type" : "text",
        "reg" : "/horse/(\\w+)"
    },
    "gender" : {
        "col_name" : "性別",
        "selector" : "div.Horse_Info_Detail",
        "var_type" : "text",
        "reg": "[牡牝騸セ]"
    },
    "age" : {
        "col_name" : "馬齢",
        "selector" : "div.Horse_Info_Detail",
        "var_type" : "integer",
        "reg": "\\d+"
    },
    "burden" : {
        "col_name" : "斤量",
        "selector" : "span.JockeyWeight",
        "var_type" : "real",
        "reg": "\\d+.?\\d?"
    },
    "jockey_id" : {
        "col_name" : "騎手ID",
        "selector" : "td.Jockey",
        "function": {"set_url": ["jockey_id"]},
        "var_type" : "text",
        "reg" : "\\d+"
    },
    "jockey_name" : {
        "col_name" : "騎手名",
        "selector" : "td.Jockey",
        "var_type" : "text",
        "reg" : "\\S+"
    },
    "rap_time": {
        "col_name" : "タイム",
        "selector" : "span.RaceTime:first-child",
        "function": {"time_to_seconds": ["rap_time"]},
        "var_type" : "real",
        "reg" : "(\\d+.\\d+)"
    },
    "last_3f": {
        "col_name" : "後3F",
        "selector" : "td:nth-child(12)",
        "var_type" : "real",
        "reg" : "(\\d+.\\d+)"
    },
    "passage_rank": {
        "col_name" : "通過順",
        "selector" : "td.PassageRate",
        "var_type" : "text",
        "reg" : "\\d{1,2}-\\d{1,2}-\\d{1,2}-\\d{1,2}|\\d{1,2}-\\d{1,2}-\\d{1,2}|\\d{1,2}-\\d{1,2}|\\d{1,2}"
    },
    "trainer_name" : {
        "col_name" : "厩舎名",
        "selector" : "td.Trainer",
        "function": {"set_title": ["trainer_name"]},
        "var_type" : "text",
        "reg" : "\\D+"
    },
    "trainer_id" : {
        "col_name" : "厩舎ID",
        "selector" : "td.Trainer",
        "function": {"set_url": ["trainer_id"]},
        "var_type" : "text",
        "reg" : "\\d+"
    },
    "weight" : {
        "col_name" : "馬体重",
        "selector" : "td.Weight",
        "var_type" : "integer",
        "reg" : "(\\d+)\\([+-]?\\d*\\)"
    },
    "weight_diff" : {
        "col_name" : "増減",
        "selector" : "td.Weight",
        "var_type" : "integer",
        "reg" : "\\d+\\(([+-]?\\d+)\\)"
    },
    "prize" : {
        "col_name" : "獲得賞金",
        "selector" : "null",
        "var_type" : "real"
    }
}